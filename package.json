{
  "name": "hello-next",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "now-build": "npm run build",
    "analyze": "cross-env BUNDLE_ANALYZE=true next build ",
    "dev": "node server.js",
    "env": "node ./bin/env.js",
    "build": "next build ",
    "export": "rm -rf ./out && next export  && mv /out ./out",
    "start": "cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production node server.js",
    "lint": "eslint --quiet 'pages/**/*.js' 'project/**/*.js' 'components/**/*.js' 'tests/**/*.js' 'common/**/*.js'",
    "test:e2e": "cross-env E2E=true next build && cross-env NODE_ENV=production E2E=true nightwatch ./tests/e2e/index.e2e.js -c ./tests/e2e/nightwatch.conf.js",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --opts ./tests/mocha.opts",
    "test:unit:watch": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --watch --opts ./tests/mocha.opts \"./common/**/*.test.js\"",
    "test:pact": "cross-env NODE_ENV=pactTest mocha --opts ./tests/mocha.opts \"**/*.pact.js\"",
    "test:pact:server": "cross-env MOCK_SERVER=true node_modules/.bin/nodemon --watch tests --exec node ./tests/pact/helpers/pact.mock-server.js",
    "test:pact:publish": "node tests/pact/helpers/pact.publish.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
