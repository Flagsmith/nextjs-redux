    "@pact-foundation/pact-node": "^9.0.2",
    "@welldone-software/why-did-you-render": "3.3.3",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "10.0.1",
    "bootstrap": "4.3.1",
    "chromedriver": "^76.0.0",
    "classnames": "2.2.6",
    "colors": "1.3.3",
    "cookie": "0.4.0",
    "cross-env": "5.2.0",
    "date-fns": "^2.0.1",
    "dotenv": "6.2.0",
    "eslint": "6.2.2",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.6.0",
